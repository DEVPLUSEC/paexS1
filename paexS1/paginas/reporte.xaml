<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="paexS1.paginas.reporte">
    <ContentPage.Content>
        <StackLayout>
            <Frame BackgroundColor="#2196F3" Padding="24" CornerRadius="0">
                <Label x:Name="txtUsuaconec"  HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
            </Frame>

            
                
                
                <Label Text="Reporte de datos"  HorizontalTextAlignment="Center" TextColor="black" FontSize="16"/>
            
        
                <Label x:Name="datoEdad" HorizontalOptions="Center"></Label>
                <Label x:Name="datoNombre" HorizontalOptions="Center"></Label>
                <Label x:Name="datoApellido" HorizontalOptions="Center"></Label>
                <Label x:Name="datoSalario" HorizontalOptions="Center"></Label>
                <Label x:Name="datoDireccion" HorizontalOptions="Center"></Label>
           
        </StackLayout>
    </ContentPage.Content>
</ContentPage>